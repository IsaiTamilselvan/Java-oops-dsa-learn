class a{
    private int num;
    String name;
    int[] arr;

    public int getnum(){ //help us to 
        return num;
    }

    public int getnum(int num){
        this.num=num;
    }

    public a(int num,String name){
        this.num=num;
        this.name=name;
        this.arr=new int[num];
    }
      
}

public class access{
